{
  "name": "URSSAF - Pr√©visu Documents",
  "author": "Kieron AUDIFFREN",
  "description": "Ouvrez vos documents dans un nouvel onglet sur le site autoentrepreneur.urssaf.fr !",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "js": ["contentScript.js"],
      "matches": ["https://www.autoentrepreneur.urssaf.fr/*"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["webRequest"],
  "host_permissions": [
    "https://www.autoentrepreneur.urssaf.fr/*",
    "https://api.urssaf.fr/*"
  ],
  "icons": {
    "128": "/icons/128.png"
  }
}
